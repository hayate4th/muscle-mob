{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2018", "dom"],
    "jsx": "react",
    "strict": true,
    "rootDirs": [
      "src"
    ],
    "baseUrl": "./",
    "paths": {"import-png": ["types/import-png"]},
    "typeRoots": ["types", "node_modules/@types"],
    "types": [
      "react",
      "import-png"
    ],
    "esModuleInterop": true
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}